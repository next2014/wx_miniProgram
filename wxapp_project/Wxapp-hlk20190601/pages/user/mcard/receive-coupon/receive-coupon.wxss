/*优惠劵*/

.coupon-list {
    overflow: hidden;
    border-radius: 6rpx;
    box-shadow: 3px 3px 12rpx rgba(0, 0, 0, 0.05);
    background: var(--juan-color);
    position: relative;
}

.coupon-list:nth-child(n+2) {
    margin-top: 30rpx;
}

.stamp-top {
    color: #fff;
    padding: 40rpx;
    align-items: center;
}

.top-rg {
    width: 160rpx;
    height: 34px;
    line-height: 34px;
    font-size: 16px;
    border-radius: 6rpx;
    background-color: #fff;
    color: var(--juan-color);
}

.top-rg::after {
    border-radius: 6rpx;
    border: none;
}

.top-lf .f14 {
    opacity: 0.6;
}

.top-rg:active {
    opacity: 0.9;
}

.stamp-footer {
    padding: 16rpx 40rpx;
    background-color: #fff;
    justify-content: space-between;
}

.customicon-zhang {
    display: block;
    font-size: 36rpx;
    color: #000;
    padding: 6rpx 0;
    width: 172rpx;
    text-align: center;
    line-height: 1.1;
    border: 1rpx solid currentColor;
    position: relative;
    transform: rotate(25deg) scale(0.8);
}

.customicon-zhang::before, .customicon-zhang::after {
    content: '';
    border-radius: 50%;
    position: absolute;
}

.customicon-zhang::before {
    width: 120rpx;
    height: 120rpx;
    border: 1px dashed currentColor;
    top: -35rpx;
    left: 24rpx;
    opacity: 0.6;
}

.customicon-zhang::after {
    width: 150rpx;
    height: 150rpx;
    border: 1px solid currentColor;
    top: -50rpx;
    left: 9rpx;
}

.coupon-list {
    --juan-color: #bbb;
}

.coupon-list.youhui {
    --juan-color: #ffb14b;
}

.coupon-list.zhekou {
    --juan-color: #ff7256;
}

.coupon-list.manjian {
    --juan-color: #5696ff;
}
